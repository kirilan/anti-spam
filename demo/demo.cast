[4.412371, "o", "Data Deletion Assistant - Development Commands\r\n"]
[4.414306, "o", "\r\n"]
[4.416136, "o", "====== Docker (Recommended) ======\r\n"]
[4.417969, "o", "  make dev           Start development environment (Docker)\r\n"]
[4.419873, "o", "  make build         Build Docker images\r\n"]
[4.421604, "o", "  make up            Start all services\r\n"]
[4.423323, "o", "  make down          Stop all services\r\n"]
[4.425002, "o", "  make logs          View container logs\r\n"]
[4.426699, "o", "  make clean         Remove containers and volumes\r\n"]
[4.428387, "o", "\r\n"]
[4.430068, "o", "====== Local System Setup ======\r\n"]
[4.431904, "o", "  make setup         Complete local setup (env, deps, pre-commit)\r\n"]
[4.433652, "o", "  make install       Install backend dependencies (uv)\r\n"]
[4.43534, "o", "  make install-dev   Install all local dependencies (backend + frontend)\r\n"]
[4.437078, "o", "  make install-pip   Install backend via pip (alternative to uv)\r\n"]
[4.438794, "o", "  make sync-requirements  Regenerate requirements.txt from pyproject.toml\r\n"]
[4.440664, "o", "\r\n"]
[4.441526, "o", "====== Local System Development ======\r\n"]
[4.443379, "o", "  make run-backend   Run backend locally (requires local deps)\r\n"]
[4.444143, "o", "  make run-worker    Run Celery worker locally\r\n"]
[4.44573, "o", "  make run-beat      Run Celery beat locally\r\n"]
[4.447617, "o", "\r\n"]
[4.449428, "o", "====== Database ======\r\n"]
[4.45115, "o", "  make migrate       Run Alembic migrations\r\n"]
[4.453083, "o", "  make migrate-new   Create new migration (m='description')\r\n"]
[4.454693, "o", "  make db-shell      Open PostgreSQL shell (Docker)\r\n"]
[4.456504, "o", "\r\n"]
[4.45832, "o", "====== Testing & Quality ======\r\n"]
[4.460015, "o", "  make test          Run tests (local)\r\n"]
[4.46179, "o", "  make test-cov      Run tests with coverage (local)\r\n"]
[4.463499, "o", "  make lint          Run all linters via pre-commit\r\n"]
[4.465361, "o", "  make format        Format code via pre-commit\r\n"]
[4.467126, "o", "  make check         Run all checks (lint + test)\r\n"]
[4.467891, "o", "\r\n"]
